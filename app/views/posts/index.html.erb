<p id="notice"><%= notice %></p>

<h1>Photos</h1>

<%= link_to 'Upload new photo', new_post_path %><br><br>

<div class="all-cont">
    <% @posts.each do |post| %>
      <div class="image-cont">
       <%= image_tag(post.photo, height: "200")  %><br>
        <b><%= post.title%></b>
          <div class="menu-items">
            <%= link_to 'Show', post %>
            <%= link_to 'Edit', edit_post_path(post) %>
            <%= link_to 'Delete', post, method: :delete, data: { confirm: 'Are you sure?' } %>
          </div>
        </div>

    <% end %>
